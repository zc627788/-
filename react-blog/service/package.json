{
    "name": "service",
    "version": "1.0.0",
    "description": "",
    "private": true,
    "egg": {
        "declarations": true
    },
    "dependencies": {
        "bootstrap": "^3.3.7",
        "concurrently": "^5.3.0",
        "egg": "^2.15.1",
        "egg-cors": "^2.2.3",
        "egg-mysql": "^3.0.0",
        "egg-scripts": "^2.11.0",
        "react": "^17.0.1",
        "wait-on": "^5.2.1"
    },
    "devDependencies": {
        "autod": "^3.0.1",
        "autod-egg": "^1.1.0",
        "egg-bin": "^4.11.0",
        "egg-ci": "^1.11.0",
        "egg-mock": "^3.21.0",
        "eslint": "^5.13.0",
        "eslint-config-egg": "^7.1.0"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "scripts": {
        "start": "concurrently \"npm run client\" \"npm run admin\" \"npm run dev\"",
        "stop": "egg-scripts stop --title=egg-server-service",
        "dev": "egg-bin dev",
        "debug": "egg-bin debug",
        "client": "npm run dev --prefix blog",
        "admin": "npm run start --prefix admin",
        "test": "npm run lint -- --fix && npm run test-local",
        "test-local": "egg-bin test",
        "cov": "egg-bin cov",
        "lint": "eslint .",
        "ci": "npm run lint && npm run cov",
        "autod": "autod"
    },
    "ci": {
        "version": "10"
    },
